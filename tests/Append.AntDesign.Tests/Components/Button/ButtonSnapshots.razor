<SnapshotTest Description="Loading" Setup="(SnapshotTest test) => test.Services.AddSingleton<IIconService,IconServiceMock>()">
    <TestInput>
        <Button Type="ButtonType.Primary" Loading Label="Loading" />
        <Button Type="ButtonType.Primary" Size="Size.Small" Loading Label="Loading" />
        <Button Type="ButtonType.Primary" Loading>
            <Icon Type="IconType.Outlined.Poweroff" />
        </Button>
        <br />
        <Button Type="ButtonType.Primary" Loading="isLoading1" OnClick="startLoading1" Label="Click me!" />
        <Button Type="ButtonType.Primary" Loading="isLoading2" OnClick="startLoading2" Label="Click me!">
            <Icon Type="IconType.Outlined.Poweroff" />
        </Button>
        <Button Type="ButtonType.Primary" Loading="isLoading3" OnClick="startLoading3">
            <Icon Type="IconType.Outlined.Poweroff" />
        </Button>
        @code{
            bool isLoading1 = false;
            bool isLoading2 = false;
            bool isLoading3 = false;
            async Task startLoading1()
            {
                isLoading1 = true;
                await Task.Delay(3000);
                isLoading1 = false;
            }
            async Task startLoading2()
            {
                isLoading2 = true;
                await Task.Delay(3000);
                isLoading2 = false;
            }
            async Task startLoading3()
            {
                isLoading3 = true;
                await Task.Delay(3000);
                isLoading3 = false;
            }
        }
    </TestInput>
    <ExpectedOutput>
        <button type="button" class="ant-btn ant-btn-primary ant-btn-loading">
            <span class="ant-btn-loading-icon">
